{
  "name": "csshint",
  "description": "lint your css code",
  "version": "0.2.7",
  "keywords": [
    "csslint",
    "csshint"
  ],
  "maintainers": [
    {
      "name": "ielgnaw",
      "email": "wuji0223@gmail.com"
    }
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "edp-core": "^1.0.32",
    "es6-promise": "^3.0.2",
    "postcss": "^5.0.10",
    "q": "^1.4.1"
  },
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "fecs": "*",
    "istanbul": "^0.3.2",
    "jasmine-node": "^1.14.5"
  },
  "scripts": {
    "lint": "fecs lib test/**/*.spec.js --type=js",
    "coverage": "istanbul cover jasmine-node --captureExceptions test/**/*.spec.js",
    "test": "npm run lint && npm run coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "main": "./lib/checker.js",
  "bin": {
    "csshint": "./bin/csshint-cli"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecomfe/node-csshint"
  }
}
