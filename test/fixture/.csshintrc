{
    "max-error": 1000,
    "no-bom": true,
    "require-before-space": ["{"],
    "require-after-space": [":", ","],
    "max-length": 120,
    "disallow-important": true,
    "require-around-space": [ "+", "~", ">"],
    "max-selector-nesting-level": 3,
    "require-newline": ["selector", "property", "media-query-condition"]
}

